# - 1 задча длина слова
print (len('What is your name? '))

# - 2 суммы и разности
first=2
second=6
summa=(first+second)
diff=(first-second)
print(summa,diff)

# - 3 среднее арифметическое
first =4
sekond =8
thirt =12
mean=(first+sekond+thirt)
print(mean//3)

# - 4 простые строчки
first_string = 'Вторник'
second_string = 'Понедельник'
print(second_string + ', ' + first_string)

# - 5 сложная форумула
a=5
b=10
c=15
f=(a*b)+(a*c)
d=f**3
print(d/2)
